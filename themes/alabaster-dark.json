{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Alabaster Dark",
  "author": "Liffindra Angga Zaaldian",
  "themes": [
    {
      "name": "Alabaster Dark",
      "appearance": "dark",
      "style": {
        "background": "#0e1415ff",
        "border": "#323738ff",
        "created": "#6abf40ff",
        "deleted": "#d2322dff",
        "editor.active_line.background": "#ffffff10",
        "editor.background": "#0e1415ff",
        "editor.document_highlight.read_background": "#293334ff",
        "editor.foreground": "#cececeff",
        "editor.gutter.background": "#0e1415ff",
        "element.hover": "#575b5cff",
        "element.selected": "#575b5cff",
        "elevated_surface.background": "#24292aff",
        "error": "#cc3333ff",
        "error.background": "#2b1d1eff",
        "error.border": "#00000000",
        "hint": "#cecece7f",
        "hint.background": "#4a88e4ff",
        "hint.border": "#00000000",
        "ignored": "#878a8aff",
        "info": "#4a88e4ff",
        "info.background": "#24292aff",
        "modified": "#ec8013ff",
        "panel.background": "#323738ff",
        "panel.focused_border": "#575b5cff",
        "players": [
          {
            "cursor": "#cd974bff",
            "selection": "#293334ff"
          }
        ],
        "scrollbar.thumb.background": "#505555ff",
        "scrollbar.thumb.hover_background": "#505555ff",
        "scrollbar.track.background": "#ffffff10",
        "search.match_background": "#cd974bff",
        "status_bar.background": "#24292aff",
        "syntax": {
          "boolean": {
            "color": "#cc8bc9ff"
          },
          "comment": {
            "color": "#dfdf8eff"
          },
          "constant": {
            "color": "#cc8bc9ff"
          },
          "constructor": {
            "color": "#71ade7ff"
          },
          "function": {
            "color": "#71ade7ff"
          },
          "number": {
            "color": "#cc8bc9ff"
          },
          "operator": {
            "color": "#708b8dff"
          },
          "punctuation": {
            "color": "#708b8dff"
          },
          "punctuation.bracket": {
            "color": "#cececeff"
          },
          "string": {
            "color": "#95cb82ff"
          },
          "string.escape": {
            "color": "#ffffff10"
          },
          "type": {
            "color": "#71ade7ff"
          }
        },
        "tab.active_background": "#0e1415ff",
        "tab.inactive_background": "#323738ff",
        "tab_bar.background": "#323738ff",
        "terminal.ansi.black": "#000000ff",
        "terminal.ansi.bright_black": "#777777ff",
        "terminal.ansi.red": "#e25d56ff",
        "terminal.ansi.bright_red": "#f36868ff",
        "terminal.ansi.green": "#73ca50ff",
        "terminal.ansi.bright_green": "#88db3fff",
        "terminal.ansi.yellow": "#e9bf57ff",
        "terminal.ansi.bright_yellow": "#f0bf7aff",
        "terminal.ansi.blue": "#4a88e4ff",
        "terminal.ansi.bright_blue": "#6f8fdbff",
        "terminal.ansi.magenta": "#915cafff",
        "terminal.ansi.bright_magenta": "#e987e9ff",
        "terminal.ansi.cyan": "#23acddff",
        "terminal.ansi.bright_cyan": "#4ac9e2ff",
        "terminal.ansi.white": "#cececeff",
        "terminal.ansi.bright_white": "#ffffffff",
        "terminal.background": "#0e1415ff",
        "text.disabled": "#878a8aff",
        "text.muted": "#cececeff",
        "title_bar.background": "#323738ff",
        "title_bar.inactive_background": "#323738ff",
        "toolbar.background": "#0e1415ff",
        "warning": "#e9bf57ff",
        "warning.background": "#ad8501ff",
        "warning.border": "#00000000"
